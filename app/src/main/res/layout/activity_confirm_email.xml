<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="wrap_content">
	
	<TextView
		android:id="@+id/yeetview"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="test1234"
		android:textAlignment="center"
		android:textAppearance="?attr/textAppearanceHeadline5"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintTop_toTopOf="parent"/>
	
	<com.google.android.material.button.MaterialButton
		android:id="@+id/buttong1"
		style="@style/Widget.HappyApp.OutlinedButton"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="dieser button funktioniert nicht"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/yeetview"/>
	
	<com.google.android.material.button.MaterialButton
		android:id="@+id/buttong2"
		style="@style/Widget.HappyApp.ContainedButton"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="der auch nicht"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/buttong1"/>
</androidx.constraintlayout.widget.ConstraintLayout>